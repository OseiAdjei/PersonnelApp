@using App.Domain
@using Syncfusion.EJ2
@model IEnumerable<Nsp>

@{
    ViewData["Title"] = "National Service Personnel";
}

<div class="card-control-section card-control-section vertical_card_layout">
    <div class="e-card-resize-container">
        @if (Model != null && Model.Any())
        {
            @foreach (var item in Model)
            {
                <div class="col-xs-6 col-sm-6 col-lg-6 col-md-6">
                    <div tabindex="0" class="e-card profile" style="justify-content: flex-start;">
                        <div class="e-card-header">
                            <img src="@item.NspPicUrl" width="100" alt="@item.NspName">
                        </div>
                        <div class="e-card-header">
                            <div class="e-card-header-caption center">
                                <div class="e-card-header-title">@item.NspName</div>
                                <div class="e-card-sub-title">@item.NspEmail</div>
                            </div>
                        </div>
                        <div class="e-card-separator"></div>
                        <div class="e-card-content">
                            @item.NspBio
                        </div>
                        <div class="e-card-actions center">
                            <a class="btn btn-outline-primary float-right" asp-action="Details_Personnel" asp-route-id="@item.NspId">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-eye-fill" viewBox="0 0 16 16">
                                    <path d="M10.5 8a2.5 2.5 0 1 1-5 0 2.5 2.5 0 0 1 5 0" />
                                    <path d="M0 8s3-5.5 8-5.5S16 8 16 8s-3 5.5-8 5.5S0 8 0 8m8 3.5a3.5 3.5 0 1 0 0-7 3.5 3.5 0 0 0 0 7" />
                                </svg> Show Details
                            </a>
                            <button class="e-card-btn" title="Details"></button>
                            <button class="e-card-btn" title="Google+">
                                <span class="e-google-icon cb-icons "></span>
                            </button>
                            <button class="e-card-btn" title="Facebook">
                                <span class="e-fb-icon cb-icons "></span>
                            </button>
                            <button class="e-card-btn" title="Tweets">
                                <span class="e-tweet-icon cb-icons "></span>
                            </button>
                        </div>
                    </div>
                </div>
            }
        }
        else
        {
            <p>No personnel in records</p>
        }
    </div>
    <div style="position: fixed; left: 130px; bottom: 120px;" class="text-white">
        <a asp-controller="NSPs" asp-action="NewPersonnel" class="btn btn-success">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-plus-circle-fill" viewBox="0 0 16 16">
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0M8.5 4.5a.5.5 0 0 0-1 0v3h-3a.5.5 0 0 0 0 1h3v3a.5.5 0 0 0 1 0v-3h3a.5.5 0 0 0 0-1h-3z" />
            </svg> Add New
        </a>
    </div>
</div>


